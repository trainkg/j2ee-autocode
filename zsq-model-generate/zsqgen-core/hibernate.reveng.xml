<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-reverse-engineering SYSTEM
	"http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"
			length="32" not-null="true">
		</sql-type>
	</type-mapping>
	
	<table-filter match-name="org_user" match-catalog="jxkh">
		<!-- 全局参数 -->
		<meta attribute="aa">123</meta>
		<meta attribute="bb">312</meta>
	</table-filter>
	<table catalog="jxkh" name="org_user">
		<column name="email">
			<meta attribute="picker">true</meta>
		</column>
		<column name="user_level">
			<meta attribute="picker">true</meta>
		</column>
	</table>
</hibernate-reverse-engineering>